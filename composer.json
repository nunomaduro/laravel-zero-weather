{
    "name": "nunomaduro/laravel-zero",
    "description": "Laravel Zero provides the perfect starting point for your next Laravel Console Application.",
    "keywords": ["framework", "laravel", "console", "zero"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/nunomaduro/laravel-zero/issues",
        "source": "https://github.com/nunomaduro/laravel-zero"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "kitetail/zttp": "^0.1.0",
        "nunomaduro/zero-framework": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "php application rename"
        ]
    },
    "bin": ["weather"]
}
